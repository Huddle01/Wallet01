{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "@wallet01/cosmos#build": {
      "dependsOn": ["@wallet01/core#build"]
    },
    "@wallet01/solana#build": {
      "dependsOn": ["@wallet01/core#build"]
    },
    "@wallet01/evm#build": {
      "dependsOn": ["@wallet01/core#build"]
    },
    "@wallet01/tezos#build": {
      "dependsOn": ["@wallet01/core#build"]
    },
    "@wallet01/react#build": {
      "dependsOn": ["@wallet01/core#build"],
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  }
}
